{
  "name": "seminar-registration-backend",
  "version": "1.0.0",
  "description": "Backend for Prompt Your Future Seminar Registration",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "dev:full": "concurrently \"npm run dev\" \"cd frontend && npm start\"",
    "install:all": "npm install && cd frontend && npm install",
    "build": "cd frontend && npm run build",
    "2fa:help": "node scripts/2fa-cli.js help",
    "2fa:generate": "node scripts/2fa-cli.js generate",
    "2fa:verify": "node scripts/2fa-cli.js verify",
    "2fa:token": "node scripts/2fa-cli.js token"
  },
  "keywords": [
    "seminar",
    "registration",
    "express",
    "nodejs"
  ],
  "author": "HARSHAD NIKAM",
  "watermark": "Developed by HARSHAD NIKAM",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "http-proxy-middleware": "^3.0.5",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.16.5",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "qrcode": "^1.5.4",
    "speakeasy": "^2.0.0",
    "sqlite3": "^5.1.6",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "concurrently": "^8.0.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
